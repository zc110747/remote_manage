echo "boot.src init!"
setenv serverip "192.168.3.29" 
setenv ipaddr "192.168.3.99" 
setenv netmask "255.255.255.0" 
setenv gateway "192.168.3.1"
setenv nfspath "/home/freedom/Desktop/sdk/arm/build/nfs_root"
